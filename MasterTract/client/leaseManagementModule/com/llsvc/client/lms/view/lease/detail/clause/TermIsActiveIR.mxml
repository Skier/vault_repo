    horizontalScrollPolicy="off" verticalScrollPolicy="off">

    <mx:Script>
        <![CDATA[
        	import com.llsvc.domain.LeaseClause;
        
            [Embed(source="assets/icons16/tick.png")]
            [Bindable]
            private var imgComplete:Class;
        ]]>
    </mx:Script>
    
    <mx:Image source="{imgComplete}" width="16" height="16" visible="{LeaseClause(data).isActive}"/>
    
</mx:HBox>

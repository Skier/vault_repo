    <!-- Payment parameters -->    
    <add key="AccountSiteUrl" value="http://localhost/Dpi.Central.Web.Account"/>
    <add key="PublicSiteUrl" value="http://localhost/Dpi.Central.Web"/>
 </appSettings>

</configuration>

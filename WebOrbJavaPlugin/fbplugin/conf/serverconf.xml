        <excludedClass>org.idoox.webservice.server.LifeCycleService</excludedClass>

        <excludedClass>org.idoox.webservice.server.WebServiceContext</excludedClass>
        <excludedClass>org.systinet.wasp.webservice.*</excludedClass>
        <!-- Remote references -->
        <internalSchema fileName="com/idoox/wasp/interrefs/interrefsEncoded.xsd" targetNamespace="http://idoox.com/interface" use="encoded"/>
        <internalSchema fileName="com/idoox/wasp/interrefs/interrefsLiteral.xsd" targetNamespace="http://idoox.com/interface" use="literal"/>
        <!-- Security -->
        <internalSchema fileName="com/idoox/wasp/security/securityEncoded.xsd" targetNamespace="http://idoox.com/security" use="encoded"/>
        <internalSchema fileName="com/idoox/wasp/security/securityLiteral.xsd" targetNamespace="http://idoox.com/security" use="literal"/>
        <!-- IDOOX containers -->
        <internalSchema fileName="com/idoox/wasp/serialization/javabuiltin/idooxContainers.xsd" targetNamespace="http://idoox.com/containers" use="encoded"/>
        <!-- Apache compatible containers -->
        <internalSchema fileName="com/idoox/wasp/serialization/javabuiltin/apacheContainers.xsd" targetNamespace="http://idoox.com/containers" use="encoded"/>
        <internalSchema fileName="com/idoox/wasp/serialization/javabuiltin/apacheContainers.xsd" targetNamespace="http://xml.apache.org/xml-soap" use="encoded"/>
        <!-- MS .NET compatible containers for RPC/ENC -->
        <internalSchema fileName="com/idoox/wasp/serialization/javabuiltin/msnet/msnetEncodedContainers.xsd" targetNamespace="http://systinet.com/containers/encoded/ms.net" use="encoded"/>
        <!-- MS .NET compatible containers for DOC/LIT -->
        <internalSchema fileName="com/idoox/wasp/serialization/xml/msnet/msnetLiteralContainers.xsd" targetNamespace="http://systinet.com/containers/literal/ms.net" use="literal"/>
        <!-- Sparse Arrays -->
        <internalSchema fileName="com/idoox/wasp/serialization/idooxbuiltin/array.xsd" targetNamespace="http://idoox.com/sparse-array" use="encoded"/>
        <!-- reference schema is used in DIME ReferencedBinary types -->
        <internalSchema fileName="com/idoox/xml/schema/known_schemas/reference.xsd" targetNamespace="http://schemas.xmlsoap.org/ws/2002/04/reference/" use="literal"/>
        <!-- reference schema is used in DIME ReferencedBinary types -->
        <internalSchema fileName="com/idoox/xml/schema/known_schemas/reference.xsd" targetNamespace="http://schemas.xmlsoap.org/ws/2002/04/reference/" use="encoded"/>
    </java2Type>

    <securityPreferences name="main">
        <pstoreInitParams/>
        <accessController providerClass="com.systinet.security.policy.PolicyAccessController"/>
        <!--property name="DynamicSecurityProviders" value=""/-->
        <property name="pstore.source.config.file" value="${.}/conf/pstore.xml"/>
        <securityProvider providerClass="com.idoox.wasp.security.ssl.SSLServerProvider" providerName="SSL"/>
        <securityProvider providerClass="com.idoox.wasp.security.http.basic.HttpBasicServerProvider" providerName="HttpBasic"/>
        <securityProvider providerClass="com.idoox.wasp.security.http.digest.HttpDigestServerProvider" providerName="HttpDigest"/>
        <securityProvider providerClass="com.idoox.wasp.security.kerberos.KrbServerProvider" providerName="Kerberos"/>
        <securityProvider providerClass="com.systinet.wasp.security.ws.provider.WSSecurityServerProvider" providerName="WS-Security"/>
        <securityProvider providerClass="com.idoox.wasp.security.siteminder.SmServerProvider" providerName="Siteminder"/>
        <updateablePolicy providerClass="com.systinet.security.policy.xml.XMLUpdateablePolicy">
            <property name="wasp.policy.xml.file" value="${.}/conf/wasp.policy"/>
        </updateablePolicy>
    </securityPreferences>

    <httpsPreferences name="https">
        <alias>sslalias</alias>
        <defaultTimeout>40000</defaultTimeout>
        <maxIdleTime>500</maxIdleTime>
        <maxReadTime>40000</maxReadTime>
        <maxThreads>255</maxThreads>
        <minThreads>5</minThreads>
        <needsClientAuth>false</needsClientAuth>
        <nonProxyHosts>localhost</nonProxyHosts>
        <password>changeit</password>
        <port>6443</port>
    </httpsPreferences>

    <module loader="com.idoox.wasp.InterfaceIntrospector" name="Interface Introspector"/>

    <xmlmime name="resolved_xmlmime_specifications">
        <resolve namespace="http://www.w3.org/2004/11/xmlmime">
	    <descriptionAttribute>contentType</descriptionAttribute>
	    <annotationAttribute>expectedMediaType</annotationAttribute>
	</resolve>
        <resolve namespace="http://www.w3.org/2005/05/xmlmime">
	    <descriptionAttribute>contentType</descriptionAttribute>
	    <annotationAttribute>expectedContentTypes</annotationAttribute>
	</resolve>
    </xmlmime>

</config>

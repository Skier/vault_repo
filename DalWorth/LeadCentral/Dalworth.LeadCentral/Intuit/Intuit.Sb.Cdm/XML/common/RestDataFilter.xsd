    <xs:complexType name="AbstractRequest" abstract="true">
        <xs:annotation>
            <xs:documentation>Request abstract base type.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:group ref="RequestParams" minOccurs="0"/>

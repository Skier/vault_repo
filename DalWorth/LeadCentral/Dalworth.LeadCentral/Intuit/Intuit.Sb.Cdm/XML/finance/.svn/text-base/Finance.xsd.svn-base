	<xs:complexType name="CreditCardRefund">
		<xs:annotation>
			<xs:documentation>Financial transaction representing a refund of a credit card sale transaction, i.e. refunding the invoice, or sales receipt, or a statement charge using a customer's credit card.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CdmBase">
				<xs:sequence>
					<xs:element name="Header" type="CreditCardRefundHeader" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

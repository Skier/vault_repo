	<xs:complexType name="SalesOrderLine">
		<xs:annotation>
			<xs:documentation>Provides the line level detail of the items ordered by the SalesOrder and the associated detail.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="LineSales">
				<xs:sequence>
					<xs:element name="ManuallyClosed" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The item on that line is marked as if it was fully invoiced, but it is closed as e.g. not longer available, or the customer changed his/her mind about the quantity.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SalesReceipt">
		<xs:annotation>
			<xs:documentation>Financial transaction representing the sale of goods and/or services where payment in full is received in at the time of the sale.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CdmBase">
				<xs:sequence>
					<xs:element name="Header" type="SalesReceiptHeader" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Provides information that pertains to the entire Sales Receipt</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Line" type="SalesReceiptLine" minOccurs="0"
						maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Provides line level detail of items (goods/services) sold on the SalesReceipt and associated detail</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TaxLine" type="TaxLine" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Provides the line level detail on a specific tax collected on all taxable lines on the entire transaction.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SalesReceiptHeader">
		<xs:annotation>
			<xs:documentation>Provides information that pertains to the entire Sales Receipt</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="HeaderSales">
				<xs:sequence>
					<xs:element name="ShipAddr" type="PhysicalAddress" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Identifies the address where the goods must be shipped. In a lot of cases, this is the same as the Billing Address. In multi-organization purchasing, the Shipping address may be different from the Billing address.[br /]
[b][i]QuickBooks Note[/i][/b] [br /]
If ShipAddr is not specified, and a default ship-to address is specified in QuickBooks for this customer, the default ship-to address will be used by QuickBooks.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:group ref="ShipMethodReferenceGroup" minOccurs="0"/>
					<xs:group ref="DepositToAccountReferenceGroup" minOccurs="0"/>
					<xs:group ref="PaymentMethodReferenceGroup" minOccurs="0"/>
					<xs:element name="Detail" type="PaymentDetail" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Detailed information about the payment for this sale.  It can be cash, check, or credit card.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:group ref="QboDiscountGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Specifies the discount-related data: amount or rate, account.
QBO only field.
							</xs:documentation>
						</xs:annotation>
					</xs:group>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SalesReceiptLine">
		<xs:annotation>
			<xs:documentation>Provides line level detail of items (goods/services) sold on the SalesReceipt and associated detail</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="LineSales">
				<xs:sequence>
					<xs:element name="ServiceDate" type="xs:date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Indicates the date when the service that is billable to a customer is going to be performed</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SalesRep">

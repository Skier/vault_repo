			<xs:element name="Number" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Credit Card account number, as printed on the card.  Should not have whitespace or formatting characters.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Token" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Tokenized representation of the credit card account number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Type" type="CreditCardTypeEnum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Visa, MasterCard, American Express, etc...</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NameOnAcct" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Account holder name, as printed on the card.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CcExpirMn" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Expiration Month on card, expressed as a nu 1 = January, 2 = February, etc...</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CcExpirYr" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Expiration Year on card, expressed as a 4 digit number 1999, 2003, etc...</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="BillAddrStreet" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Credit card holder billing address of record -- the street address to which credit card statements are sent.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ZipCode" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Credit card holder billing Postal Code. 5 digits in the USA</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Cvv" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Credit card validation code (sometimes printed on back of card)
						Unsupported field.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CommercialCardCode" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Code associated with commercial cards - purchase, corporate, and business cards. Lower transaction fee rates apply when these cards are used and this field is provided.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CCTxnMode" type="CCTxnModeEnum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>specifies a credit card transaction mode used in Credit Card payment transactions. May have one of the following values: CardNotPresent [DEFAULT], CardPresent</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CCTxnType" type="CCTxnTypeEnum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies the type of credit card transaction. May have one of the following values: Authorization, Capture, Charge, Refund, VoiceAuthorization</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CreditChargeResponse">
		<xs:annotation>
			<xs:documentation>Holds credit-card transaction response information from a merchant account service, but not any credit card or payment request information - see CreditChargeInfo</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CCTransId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Unique identifier of the payment transaction. It can be used to track the status of transactions, or to search transactions. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Status" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates the status of the payment transaction. Possible values include Completed, Prepared, Sent, Unknown, Voided.
						The values are defined in the CCPaymentStatusEnum.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResultCode" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Numeric code specifying the result of the transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResultMsg" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Text specifying the result of the transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="MerchantAcctNum" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Merchant account number associated with the credit card transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CardSecurityCodeMatch" type="CCSecurityCodeMatchEnum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Result of comparing the security code supplied in the credit card transaction request with the code on file with the card issuer.
						Possible values are Pass, Fail, and NotAvailable.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AuthCode" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Code returned from the credit card processor to indicate that the charge will be paid by the card issuer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AvsStreet" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The AVS (Address Verification Service) result for the street address supplied in the transaction.  Possible values are Pass, if the information matches the information on file with the cardholder's account, Fail, or NotAvailable.
						The values are defined in the CCAVSMatchEnum.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AvsZip" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The AVS (Address Verification Service) result for the zip code supplied in the transaction.  Possible values are Pass, if the information matches the information on file with the cardholder's account, Fail, or NotAvailable.
						The values are defined in the CCAVSMatchEnum.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SecurityCode" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>CardCode or Card Id field that can be optionally provided to use additional security features of credit card authorization. It is typically a 3 digit number located on the back of most credit cards.
						For American Express, it is a 4 digit number on the front.
						Unsupported field.
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ReconBatchId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates which deposit batch the transaction belongs to. Allows for integration with Intuit MAS Service and QBFS: enables reconciliation between whats charged on credit card and what is already deposited into bank.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PaymentGroupingCode" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Code that indicates membership in a group of card types that are normally deposited together.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TxnAuthorizationTime" type="xs:dateTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Timestamp indicating the time in which the card processor authorized the transaction.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TxnAuthorizationStamp" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This value is used to support the credit card transaction reconciliation.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ClientTransID" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>An identifier returned in settlement data used to support the credit card transaction reconciliation.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CreditMemo">
		<xs:annotation>
			<xs:documentation>Financial transaction representing a refund (or credit) of payment or part of a payment for goods or services that have been sold.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CdmBase">
				<xs:sequence>
					<xs:element name="Header" type="CreditMemoHeader" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Provides information that pertains to the entire CreditMemo</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Line" type="CreditMemoLine" minOccurs="0"
						maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Provides the line level detail of the services offered or items ordered and the associated detail.  It can be aligned with the Sales Order lines.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TaxLine" type="TaxLine" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Provides the line level detail on a specific tax collected on all taxable lines on the entire transaction.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CreditMemoHeader">
		<xs:annotation>
			<xs:documentation>Information that pertains to the entire CreditMemo</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="HeaderSales">
				<xs:sequence>
					<xs:group ref="ARAccountReferenceGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>ARAccountReferenceGroup Identifies the AR Account to be used for this Credit Memo.
[b]QuickBooks Notes[/b][br /]
The AR Account should always be specified or a default will be used.</xs:documentation>
						</xs:annotation>
					</xs:group>
					<xs:group ref="SalesTermReferenceGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Identifies the payment term for an Order or Invoice. Supports the ability to specify an absolute due date, a number of days from a start date, a percent discount or an absolute discount. A common term like "Net30Days" is explicitly code as either a calculated DueDateTime or a PaymentTermsDateTime and NumberOfDays.</xs:documentation>
						</xs:annotation>
					</xs:group>
					<xs:element name="DueDate" type="xs:date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Date when the Invoice is to be paid.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BillAddr" type="PhysicalAddress" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Identifies the physical address where the bill or invoice is to be sent.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ShipAddr" type="PhysicalAddress" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Identifies the address where the goods must be shipped. In a lot of cases, this is the same as the Billing Address. In multi-organization purchasing, the Shipping address may be different from the Billing address.[br /]
[b][i]QuickBooks Note[/i][/b] [br /]
If ShipAddr is not specified, and a default ship-to address is specified in QuickBooks for this customer, the default ship-to address will be used by QuickBooks.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BillEmail" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Identifies the email address where the bill or invoice is to be sent.
[b]QuickBooks Notes[/b][br /]
[i]Unsupported field.[/i]
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:group ref="ShipMethodReferenceGroup" minOccurs="0"/>
					<xs:element name="RemainingCredit" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Indicates the total credit amount still available to apply towards the payment.
[b]QuickBooks Notes[/b][br /]
Non QB-writable.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Detail" type="PaymentDetail" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Detailed information about the payment for this sale.  It can be cash, check, or credit card.
[b]QuickBooks Notes[/b][br /]
[i]Unsupported field.[/i]
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:group ref="QboDiscountGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Specifies the discount-related data: amount or rate, account.
QBO only fields.
							</xs:documentation>
						</xs:annotation>
					</xs:group>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CreditMemoLine">
		<xs:annotation>
			<xs:documentation>A service or product sold, along with quantity, price, and other descriptive information</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="LineSales">
				<xs:sequence>
					<xs:element name="ServiceDate" type="xs:date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Indicates the date when the service that is billable to a customer is going to be performed</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CustomerMsg">
		<xs:annotation>
			<xs:documentation>A standard message to a customer that can be included at the bottom of a sales form.
[b]QuickBooks Notes[/b][br /]
[i]Unsupported type.[/i]
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CdmBase">
				<xs:sequence>
					<xs:element name="Name" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Contains the message to a customer.[br /]
Cloud Max Length: 256
[b][i]QuickBooks Note[/i][/b] [br /]
Required for the Add request. [br /]
Max Length: 101
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Active" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Whether or not active inactive customer message may be hidden from most display purposes and may not be used on financial transactions.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CustomerRef">
		<xs:group ref="CustomerReferenceGroup"/>
	</xs:complexType>
	<xs:complexType name="Deposit">
		<xs:annotation>
			<xs:documentation>Transaction recording a payment from the customer held in the Undeposited Funds account into the Bank account.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CdmBase">
				<xs:sequence>
					<xs:element name="Header" type="DepositHeader" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Provides information that pertains to the entire Deposit</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Line" type="DepositLine" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Provides the line level detail of the Deposit transaction.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DepositHeader">
		<xs:annotation>
			<xs:documentation>Information that pertains to the entire Deposit.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="HeaderBase">
				<xs:sequence>
					<xs:group ref="DepositToAccountReferenceGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>DepositToAccountReferenceGroup Identifies the Asset Account (bank account) to be used for this Deposit.
[b]QuickBooks Notes[/b][br /]
Required for the Add request. [br /]
							</xs:documentation>
						</xs:annotation>
					</xs:group>
					<xs:element name="CashBack" type="CashBackInfo" minOccurs="0"/>
					<xs:element name="TotalAmt" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Total amount of Deposit.
[b]QuickBooks Notes[/b][br /]
Non QB-writable.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DepositLine">
		<xs:annotation>
			<xs:documentation>Each deposit line describes one deposit into the account indicated by the Deposit To Account data in the header. Deposit lines are added as either received payments (described by a TxnID and possibly a TxnLineID) or as transfers from one account into another (where at least the Account data must be specified)</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="LineBase">
				<xs:sequence>
					<xs:group ref="TxnLineReferenceGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The reference to the payment transaction which is being deposited.</xs:documentation>
						</xs:annotation>
					</xs:group>
					<xs:element name="TxnType" type="TxnTypeEnum" minOccurs="0">
						<xs:annotation>
							<xs:documentation>For information purpose only - the type of the payment transaction.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:group ref="AccountReferenceGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Specifies the Account that is the source of the amount to be deposited.
[b]QuickBooks Notes[/b][br /]
Required for the Add request if there is no reference to the payment transaction specified.
							</xs:documentation>
						</xs:annotation>
					</xs:group>
					<xs:group ref="EntityReferenceGroup" minOccurs="0"/>
					<xs:group ref="PaymentMethodReferenceGroup" minOccurs="0"/>
					<xs:group ref="ClassReferenceGroup" minOccurs="0"/>
					<xs:element name="Amount" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Amount of the deposit.
[b]QuickBooks Notes[/b][br /]
Required for the Add request if the Deposit is specified as a transfer rather than the payment transaction reference.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CheckNum" type="xs:string" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Discount">
		<xs:annotation>
			<xs:documentation>Identifies the discount or allowance that the Customer is to receive off of the TotalAmount owed.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CdmBase">
				<xs:sequence>
					<xs:element name="Name" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>User recognizable name for the Discount.[br /]
Cloud Max Length: 4000
[b][i]QuickBooks Note[/i][/b] [br /]
Required for the Add request. [br /]
Max Length: 31
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Desc" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>User entered description for the Discount which may include user entered details with reason for the reduction of the amount.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Taxable" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>If true, calculate the sales tax first, and then apply the discount. If false, subtract the discount first and then calculate the sales tax.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Active" type="xs:boolean" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Whether or not active inactive discounts may be hidden from most display purposes and may not be used on financial transactions.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:choice minOccurs="0">
						<xs:element name="Percent" type="xs:decimal" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Percentage by which the amount due is reduced, from 0% to 100%. To enter a discount of 8.5% use 8.5, not 0.085.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Amount" type="Money" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Amount by which the amount due is reduced.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
					<xs:group ref="DiscountAccountReferenceGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Indicates the account used to track discounts given to customers on sales. When an income account is used to track discounts on sales, it is usually called a "contra-income" account.
[b][i]QuickBooks Note[/i][/b] [br /]
Required for the Add request.
							</xs:documentation>
						</xs:annotation>
					</xs:group>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EmployeeRef">
		<xs:group ref="EmployeeReferenceGroup"/>
	</xs:complexType>
	<xs:complexType name="Estimate">
		<xs:annotation>
			<xs:documentation>A proposal for a FinancialTransaction from a business to a customer for goods or services proposed to be sold, including proposed pricing.  Aka a quote or estimate.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CdmBase">
				<xs:sequence>
					<xs:element name="Header" type="EstimateHeader" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Information that pertains to the entire Estimate</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Line" type="EstimateLine" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>A service or product proposed to be sold along with quantity, price and other descriptive information.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TaxLine" type="TaxLine" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Provides the line level detail on a specific tax collected on all taxable lines on the entire transaction.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EstimateHeader">
		<xs:annotation>
			<xs:documentation>Information that pertains to the entire Estimate</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="HeaderSales">
				<xs:sequence>
					<xs:group ref="SalesTermReferenceGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Identifies the payment term for an Order or Invoice. Supports the ability to specify an absolute due date, a number of days from a start date, a percent discount or an absolute discount. A common term like "Net30Days" is explicitly code as either a calculated DueDateTime or a PaymentTermsDateTime and NumberOfDays.</xs:documentation>
						</xs:annotation>
					</xs:group>
					<xs:element name="BillAddr" type="PhysicalAddress" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Identifies the physical address where the bill or invoice is to be sent.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ShipAddr" type="PhysicalAddress" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Identifies the address where the goods must be shipped. In a lot of cases, this is the same as the Billing Address. In multi-organization purchasing, the Shipping address may be different from the Billing address.[br /]
[b][i]QuickBooks Note[/i][/b] [br /]
If ShipAddr is not specified, and a default ship-to address is specified in QuickBooks for this customer, the default ship-to address will be used by QuickBooks.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BillEmail" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Identifies the email address where the bill or invoice is to be sent.
[b]QuickBooks Notes[/b][br /]
[i]Unsupported field.[/i]
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:group ref="QboDiscountGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Specifies the discount-related data: amount or rate, account.
QBO only fields.
							</xs:documentation>
						</xs:annotation>
					</xs:group>
					<xs:element name="ExpirationDate" type="xs:date" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Date by which estimate must be accepted before invalidation.
QBO only field.
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AcceptedBy" type="xs:string" minOccurs="0">

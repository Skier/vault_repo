			<xs:element name="BackupWithholdingRate" type="percent" minOccurs="0"/>
			<xs:group ref="EffectiveGroup" minOccurs="0"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="WorkerComplianceGroup">
		<xs:sequence>
			<xs:element name="Withholding" type="Withholding" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="EmploymentVerification" type="EmploymentVerification" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="WorkerInfoGroup">
		<xs:sequence>
			<xs:element name="WorkerType" type="workerType">
				<xs:annotation>
					<xs:documentation>Worker status with respect to pyaroll laws, e.g., 1099, regular employee, statutory employee, etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="HireDate" type="xs:date" minOccurs="0"/>
			<xs:element name="ReleaseDate" type="xs:date" minOccurs="0"/>
			<xs:element name="WorkerId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Employer assigned displayable worker identifier, e.g., worker number (employee number)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="JobPosition" type="xs:string" minOccurs="0"/>
			<xs:element name="WorkersCompCode" type="xs:string" minOccurs="0"/>
			<xs:element name="LastRaiseDate" type="xs:date" minOccurs="0"/>
			<xs:element name="Active" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Useful for seasonal employers</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NameOnCheck" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Name to be printed on checks.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="HomeOfficeUser" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>If true, use HOME address to create new work location</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RelationshipToEmployer" type="relationshipToEmployer" minOccurs="0"/>
			<xs:element name="OrgRef" type="referenceId" minOccurs="0">
				<xs:annotation>
					<xs:documentation>In the case of NonEmployee, reference to the business s/he is working for.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ReadyForPay" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether the setup process for this Worker has reached a point at which we have all necessary data needed to be able to issue a paycheck.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ReadyForTax" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether the setup process for this Worker has reached a point at which we have all necessary data needed to assign payroll tax items.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:group name="WorkLocationGroup">
		<xs:sequence>
			<xs:element name="Name" type="xs:string" minOccurs="0"/>
			<xs:element name="Default" type="xs:boolean" minOccurs="0"/>
			<xs:element name="HomeOffice" type="xs:boolean" minOccurs="0"/>
			<xs:group ref="ContactInfoGroup" minOccurs="0"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="UnemploymentInsItemGroup">
		<xs:sequence>
			<xs:sequence minOccurs="0">
				<xs:element name="RateAssigned" type="xs:boolean" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Acts as a NULL indicator for the rate. </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Rate" type="percent" minOccurs="0"/>
			</xs:sequence>
			<xs:group ref="EffectiveGroup" minOccurs="0"/>
		</xs:sequence>
	</xs:group>
	<!--========================================================================-->
	<!--BASE PAYROLL TYPES-->
	<xs:simpleType name="datumValueType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="NOTSET"/>
			<xs:enumeration value="MONEY"/>
			<xs:enumeration value="PERCENT"/>
			<xs:enumeration value="QUANTITY"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:group name="PayrollDatumGroup">
		<xs:sequence>
			<xs:element name="ValueType" type="datumValueType"/>
			<xs:element name="MoneyAmount" type="Money" minOccurs="0"/>
			<xs:element name="Percentage" type="percent" minOccurs="0"/>
			<xs:element name="Quantity" type="xs:double" minOccurs="0"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="UpdatableReferenceGroup">
		<xs:annotation>
			<xs:documentation>Reference to an updatable object.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="RefId" type="id">
				<xs:annotation>
					<xs:documentation>Id of the referenced object</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RefSyncToken" type="syncToken" minOccurs="0">
				<xs:annotation>
					<xs:documentation>SyncToken for the referenced object</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:simpleType name="referenceId">
		<xs:annotation>
			<xs:documentation>Reference is only used for read-only lookups</xs:documentation>
		</xs:annotation>
		<xs:restriction base="id"/>
	</xs:simpleType>
</xs:schema>

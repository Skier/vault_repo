<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.intuit.com/sb/cdm/v2" targetNamespace="http://www.intuit.com/sb/cdm/v2" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="../common/IntuitCdmBaseTypes.xsd"/>
	<xs:include schemaLocation="../common/IntuitCustomFieldCdmTypes.xsd"/>
	<xs:include schemaLocation="../customer/PartyCdmTypes.xsd"/>
	<xs:include schemaLocation="./PayrollItem.xsd"/>
	<!--========================================================================-->
	<!--KEY PAYROLL TYPES-->
	<xs:complexType name="Employer">
		<xs:annotation>
			<xs:documentation>An singleton object representing this business (company) in the payroll domain. </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CdmBase">
				<xs:group ref="OrganizationGroupTemp" minOccurs="0"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EmployerComplianceItems">
		<xs:annotation>
			<xs:documentation>RefId and RefSyncToken identify Employer (a singleton), which is the Owner of the "payload" consisting of  the EmployerComplianceGroup.
</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="UpdatableReferenceGroup"/>
			<xs:sequence>
				<xs:group ref="EmployerComplianceGroup" minOccurs="0"/>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Worker">
		<xs:annotation>
			<xs:documentation>This object respresents a Worker in the employment relationship. 
There can be many Workers working for a single Employer.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CdmBase">
				<xs:sequence minOccurs="0">
					<xs:group ref="PersonGroup" minOccurs="0"/>
					<xs:group ref="WorkerInfoGroup" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="WorkerComplianceItems">
		<xs:annotation>
			<xs:documentation>RefId and RefSyncToken identify the Worker object, i.e., the Owner of the "payload" consisting of  the WorkerComplianceGroup.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="UpdatableReferenceGroup"/>
			<xs:group ref="WorkerComplianceGroup"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WorkerPaySchedules">
		<xs:annotation>
			<xs:documentation>RefId and RefSyncToken identify the Worker object, i.e., the Owner of the "payload" consisting of a collection of PaySchedule references.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="UpdatableReferenceGroup"/>
			<xs:element name="PayScheduleRef" type="PayScheduleReference" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WorkerWorkLocationRefs">
		<xs:annotation>
			<xs:documentation>RefId and RefSyncToken identify the Worker object, i.e., the Owner of the "payload" consisting of a collection of WorkLocation references.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="UpdatableReferenceGroup"/>
			<xs:element name="WorkLocationRef" type="WorkLocationReference"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Agency">
		<xs:annotation>
			<xs:documentation>The Agency represents an organization which role is payroll compliance enforcement.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CdmBase">
				<xs:group ref="AgencyGroup" minOccurs="0"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="BankAccts">
		<xs:annotation>
			<xs:documentation>The updatable object reference identifies the owner of the "payload" consisting of a collection of bank accounts.
An example of the owner is Employer or Worker.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="UpdatableReferenceGroup"/>
			<xs:element name="BankAcct" type="BankAcct" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ContactPersons">
		<xs:annotation>
			<xs:documentation>A collection of contact persons with references to related objects. 
The set of ContactPerson elements MUST share the references that are "attached". 
When the referecnces differ, contatc person info needs to be represented by another instance of this object.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ContactPerson" type="ContactPerson" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A collection of contact persons sharing an identical relationship with an employer realm.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:sequence>
				<xs:element name="EmployerRef" type="referenceId" minOccurs="0">
					<xs:annotation>
						<xs:documentation>There is only one employer within a realm.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ServiceProviderRef" type="referenceId" minOccurs="0">
					<xs:annotation>
						<xs:documentation>A person can represent not more than one organization in our model.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="BankAcctRef" type="referenceId" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>We can have a single contact for a collection of bank accounts</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="WorkerRef" type="referenceId" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>A single emergency contact person can be specified a group of workers.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="WorkLocationRef" type="referenceId" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>A single contact person can be specified for a group of work locations.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PayrollItemDtls">
		<xs:annotation>
			<xs:documentation>Worker is the Owner of this collection: the object Id and SyncToken identify the owning object.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="UpdatableReferenceGroup"/>
			<xs:group ref="PayrollItemsGroup"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PaySchedule">
		<xs:annotation>
			<xs:documentation>Payroll schedule.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CdmBase">
				<xs:sequence>
					<xs:group ref="PayrollScheduleGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>A group describing payroll schedule.</xs:documentation>
						</xs:annotation>
					</xs:group>
					<xs:element name="WorkerReference" type="WorkerReference" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>List of references for Workers to which this payroll schedule applies.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="RegulatoryIds">
		<xs:annotation>
			<xs:documentation>Employer (a singleton) is the Owner of this collection of identifiers: the object Id and SyncToken identify it as the owning object.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="UpdatableReferenceGroup"/>
			<xs:element name="RegulatoryId" type="RegulatoryId" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WorkLocation">
		<xs:annotation>
			<xs:documentation>Work location</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CdmBase">
				<xs:sequence>
					<xs:group ref="WorkLocationGroup" minOccurs="0"/>
					<xs:group ref="ContactPersonRefGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>A group representing contact person for the underlying object.</xs:documentation>
						</xs:annotation>
					</xs:group>
					<xs:element name="WorkerReference" type="WorkerReference" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Collection of references for Worker working at this location.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<!--VIEW MY PAYCHECK========================================================-->
	<xs:complexType name="Paychecks">
		<xs:annotation>
			<xs:documentation>A batch of paychecks (must be non-empty).</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Paycheck" type="Paycheck" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Paycheck">
		<xs:annotation>
			<xs:documentation>Paycheck</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CdmBase">
				<xs:sequence>
					<xs:group ref="EmployerReferenceGroup" minOccurs="0"/>
					<xs:group ref="WorkerReferenceGroup" minOccurs="0"/>
					<xs:group ref="PaycheckPreferencesGroup" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Group of attributes controlling how the paycheck info is to be rendered.</xs:documentation>
						</xs:annotation>
					</xs:group>
					<xs:element name="Header" type="PaycheckHeader"/>
					<xs:element name="Line" type="PaycheckLine" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>There must be at least one line item per paycheck: in the case of the "summary" paycheck it will hold the totals for the paycheck.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PaycheckHeader">
		<xs:annotation>
			<xs:documentation>Paycheck header</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DocNumber" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Paycheck number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TxnDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Pay date (payroll date). Also, the paycheck date. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PayPeriod" type="DateRange" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Pay period this paycheck is for.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Currency" type="currencyCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Currency code for all paycheck amounts, default is USD.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Memo" type="xs:string" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Memo values for the paycheck.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Status" type="PaycheckStatusEnum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Paycheck status</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PaycheckLine" abstract="1">
		<xs:annotation>
			<xs:documentation>Abstract base type for the paycheck line</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Id" type="IdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>OPTIONAL Unique identifier for the paycheck line.
If present, it might identify the underlying paycheck line item object. Typically, used when an update is involved. For VMP will most likely be not used.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:group ref="PayrollItemReferenceGroup" minOccurs="0"/>
			<xs:element name="SortSeq" type="xs:integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Sort sequence for lines, if sorting is needed. May be useful, when rendering the check image.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LineItemCategory" type="LineItemCategoryEnum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Line item category that can be used for grouping of related items, subtotaling, etc., as needed for paycheck rendering.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="StdPaycheckLine">
		<xs:annotation>
			<xs:documentation>"Standard" paycheck line representing a monetary amount other than totals</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PaycheckLine">
				<xs:sequence>
					<xs:element name="PayRate" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Pay rate, if applicable. E.g. hourly, piecework.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Quantity" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Number of units applicable to the PayRate, e.g., nr of hours worked.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Amount" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The line item amount. (PayRate and Quantity may have been used for its computation. </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="YTDAmount" type="xs:decimal" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The Year-To_Date amount inclusive of the Amount.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PayPeriod" type="DateRange" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Past pay period when required for correcting entries.
Needed when the item is for a past pay period and the associated hours are *not to be* included in overtime calcs.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PayerType" type="PayerTypeEnum" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Payer type, i.e., who is paying this item. May be useful for grouping items  when rendering the paycheck.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PayerName" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The name of the Payer of this item, e.g., an insurance company providing (third party) sick insurance.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PayeeType" type="PayeeTypeEnum" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Payee type, who is being paid with this item. May be useful for grouping items  when rendering the paycheck.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PayeeName" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The name of the payee, e.g,  Medical Insurance provider, garnishment agency.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PTOPaycheckLine">
		<xs:annotation>
			<xs:documentation>Paycheck line representing PTO Accrual</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PaycheckLine">
				<xs:sequence>
					<xs:element name="AccrualThisPeriod" type="PTOValue" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Accrual value for this pay period.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Available" type="PTOValue" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Total available time including value accrued for this pay period. Equal to YTDAccrued -YTDUsed.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="YTDAccrued" type="PTOValue" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Total time accrued this year including this pay period.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="YTDUsed" type="PTOValue" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Total time used this year including this pay period.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="TotalsPaycheckLine">
		<xs:annotation>
			<xs:documentation>Represents totals data on a paycheck: either the NET amount or DD amount(s).</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="PaycheckLine">
				<xs:sequence>
					<xs:element name="Totals" type="xs:decimal" minOccurs="0"/>
					<xs:element name="YTDTotals" type="xs:decimal" minOccurs="0"/>
					<xs:element name="BankAccountName" type="xs:string" minOccurs="0"/>
					<xs:element name="RoutingNum" type="xs:string" minOccurs="0"/>
					<xs:element name="AccountNum" type="xs:string" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:group name="EmployerReferenceGroup">
		<xs:annotation>
			<xs:documentation>Employer reference group. Consists of Employer reference and a group of attributes useful for VMP (View My Paycheck).</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EmployerId" type="IdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Reference to the Employer object.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EmployerName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Employer Name (typically Legal name) to be printed on the paycheck</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EmployerTaxId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Employer federal tax identification number, typically EIN </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:group ref="EmployerContactInfoGroup" minOccurs="0"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="EmployerContactInfoGroup">
		<xs:annotation>
			<xs:documentation>ContactInfoGroup for the Employer.
Based on the ContactInforGroup, however, no more than a single instance of each element is expected to be passed (and printed on the paycheck). </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:group ref="ContactInfoGroup" minOccurs="0"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="WorkerReferenceGroup">
		<xs:annotation>
			<xs:documentation>Worker reference group. Consists od Worker id and a group of attributes useful for VMP (View My Paycheck).</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="WorkerId" type="IdType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Reference to the Worker object</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WorkerName" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Worker name to be printed on check</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WorkerNum" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Worker Number (Employee Number), typically a number, issued by the Employer. May appear on a badge or be used to identify Worker's records.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WorkerAddress" type="PhysicalAddress" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Worker's address printed on paycheck.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WorkerTaxId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Workers's tax id, typically, SSN.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WorkerTaxationStatus" type="WorkerTaxationStatus" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>For each applicable jurisdiction, taxation information from W-4 (or equivalent), e.g., exempt, withholding status, number of exemptions, etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WorkerHireDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Hire date if it is to be printed on the paycheck.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WorkerReleaseDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Worker termination date. (May be printed on the last paycheck.)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:group name="PaycheckPreferencesGroup">
		<xs:annotation>
			<xs:documentation>Preferences determining what paycheck data to display and how</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ShowPTOData" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A flag which controls whether PTO (Paid Time Off) data is to be shown on the paycheck. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PTODisplayFormat" type="PTODisplayFormatEnum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Determines format for PTO data display.
Also, see PTOValue. If conversion from decimal to HHH:MM format truncation/rounding may be needed.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:simpleType name="PTODisplayFormatEnum">
		<xs:annotation>
			<xs:documentation>Determines the display format for PTO values</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="DECIMAL"/>
			<xs:enumeration value="HHMM"/>
			<xs:enumeration value="ASASTRING"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LineItemCategoryEnum">
		<xs:annotation>
			<xs:documentation>Enumeration of lineitem categories useful for grouping items for display purposes</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="EARNINGS"/>
			<xs:enumeration value="PRETAXDEDUCT"/>
			<xs:enumeration value="TAXFED"/>
			<xs:enumeration value="TAXSTATE"/>
			<xs:enumeration value="TAXLOCAL"/>
			<xs:enumeration value="AFTERTAXADJ"/>
			<xs:enumeration value="NETAMT"/>
			<xs:enumeration value="DIRDEP"/>
			<xs:enumeration value="NONTAXCONTRIB"/>
			<xs:enumeration value="GARNISHMENT"/>
			<xs:enumeration value="NONTAXREIMBURSEMENT"/>
			<xs:enumeration value="TAXREIMBURSEMENT"/>
			<xs:enumeration value="PTOVAC"/>
			<xs:enumeration value="PTOSICK"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PaycheckStatusEnum">
		<xs:annotation>
			<xs:documentation>Defines paycheck statys, e.g., voided, paid, etc.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="VOIDED"/>
			<xs:enumeration value="PAID"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PayeeTypeEnum">
		<xs:annotation>
			<xs:documentation>Defines Payee type</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="EMPLOYEE"/>
			<xs:enumeration value="EMPLOYER"/>
			<xs:enumeration value="3RDPARTY"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PayerTypeEnum">
		<xs:annotation>
			<xs:documentation>Defines Payer type</xs:documentation>
		</xs:annotation>
